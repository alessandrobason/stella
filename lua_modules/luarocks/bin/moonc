exec '/usr/bin/lua5.1' -e 'package.path="/home/runner/luaprojectmanager/lua_modules/share/lua/5.1/?.lua;/home/runner/luaprojectmanager/lua_modules/share/lua/5.1/?/init.lua;/usr/local/share/lua/5.1/?.lua;/usr/local/share/lua/5.1/?/init.lua;"..package.path; package.cpath="/home/runner/luaprojectmanager/lua_modules/lib/lua/5.1/?.so;/usr/local/lib/lua/5.1/?.so;"..package.cpath' -e 'local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("moonscript","0.5.0-1")' '/home/runner/luaprojectmanager/lua_modules/lib/luarocks/rocks/moonscript/0.5.0-1/bin/moonc' "$@"
